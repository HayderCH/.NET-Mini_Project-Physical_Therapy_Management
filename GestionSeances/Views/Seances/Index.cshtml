@model IEnumerable<Seance>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HeureS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypeSoin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kine)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Patient)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HeureS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeSoin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kine.NomK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Patient.Nomp)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SeanceId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SeanceId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SeanceId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
